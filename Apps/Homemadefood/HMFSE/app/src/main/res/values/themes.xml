<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.HMFSE" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>

    <style name="ToolbarTheme" parent="Widget.MaterialComponents.Toolbar">
        <item name="android:background">@color/yellow</item>
        <item name="android:popupTheme">@style/ThemeOverlay.MaterialComponents.Light</item>
        <item name="android:theme">@style/ThemeOverlay.MaterialComponents.Dark.ActionBar</item>
    </style>
</resources>